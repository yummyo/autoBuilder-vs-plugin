{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "outDir": "./out",
      "rootDir": "./src",
      "lib": ["es6"],
      "sourceMap": true,
      "skipLibCheck": true,
      "strict": false,
      "types": ["node", "vscode"]  // 确保包含 vscode 类型
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules", "out"]
  }
  